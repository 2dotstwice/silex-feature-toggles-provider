{
  "name": "2dotstwice/silex-feature-toggles-provider",
  "description": "Feature toggles for Silex",
  "license": "MIT",
  "require": {
    "qandidate/toggle": "0.4.0",
    "silex/silex": "~1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "squizlabs/php_codesniffer": "~2.3",
    "phing/phing": "~2.11",
    "satooshi/php-coveralls": "~0.7"
  },
  "autoload": {
    "psr-4": {
      "TwoDotsTwice\\SilexFeatureToggles\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TwoDotsTwice\\SilexFeatureToggles\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}
